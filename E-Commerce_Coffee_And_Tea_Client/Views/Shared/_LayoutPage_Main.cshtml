<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title</title>
    <link href="https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css" />
    <link href="~/Assets/css/output.css" rel="stylesheet" />
    <link href="~/Assets/css/style.css" rel="stylesheet" />
    @RenderSection("LibraryCSS", required: false)
</head>
<body>
    @{
        Html.RenderPartial("_PartialPage_Header");
    }

    @{
        Html.RenderPartial("_PartialPage_MenuNavigation");
    }

    @RenderSection("Banner", required: false)

    @RenderBody()

    @{
        Html.RenderPartial("_PartialPage_Footer");
    }
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js" integrity="sha512-v2CJ7UaYy4JwqLDIrZUI/4hqeoQieOmAZNXBeQyjo21dadnwR+8ZaIJVT8EE2iyI61OV8e6M8PP2/4hpQINQ/g==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    @RenderSection("LibraryJS", required: false)
</body>
</html>
